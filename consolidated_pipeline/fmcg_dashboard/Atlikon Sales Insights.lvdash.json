{
  "datasets": [
    {
      "name": "7581d5a0",
      "displayName": "vw_fact_orders_enriched",
      "queryLines": [
        "SELECT * FROM fmcg.gold.vw_fact_orders_enriched\n",
        "where channel is not NULL"
      ]
    },
    {
      "name": "9ef838da",
      "displayName": "Top 10 categories by revenue",
      "queryLines": [
        "SELECT category,SUM(total_amount_inr) as total_sales ,dense_rank()over(order by SUM(total_amount_inr) desc) AS rankings\n",
        "FROM fmcg.gold.vw_fact_orders_enriched\n",
        "GROUP BY category\n",
        "QUALIFY rankings <=10\n",
        "ORDER BY rankings"
      ]
    },
    {
      "name": "76fa94af",
      "displayName": "Monthly Sales Trends",
      "queryLines": [
        "Select division,year_quarter,SUM(total_amount_inr) as Quarterly_Revenue\n",
        "From fmcg.gold.vw_fact_orders_enriched\n",
        "Group by division,year_quarter\n",
        "order by year_quarter asc\n",
        "\n"
      ]
    }
  ],
